{
    "manifest_version": 2,
    "name": "Short Search",
    "version": "0.1",
    "omnibox": {
        "keyword": "ss"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "icons": {
        "16": "images/ss_16.png",
        "48": "images/ss_48.png",
        "128": "images/ss_128.png"
    },
    "permissions": [
        "storage"
    ]
}